{
    "FILE CB": {
    "scope": "",
    "prefix": "Phi",
    "body": [

"long long Phi(long long n) {",
"",
"    long long res = n;",
"    for (int i=2;i<=sqrt(n);i++)",
"        if (n%i==0) {",
"",
"            res-=res/i;",
"            while (n%i==0) ",
"                n/=i;",
"            ",
"        }",
"",
"    if (n>1)",
"        res-=res/n;",
"",
"    return res;",
"",
"}",

    ],
    "description": "template"
    }
}
